            <Button fx:id="sButton" alignment="CENTER" mnemonicParsing="false" onAction="#showSleep" prefHeight="115.0" prefWidth="298.0" style="-fx-background-color: gold;" text="Sleep" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="29.0" />
