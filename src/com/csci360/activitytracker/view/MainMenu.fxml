          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="lgButton" alignment="CENTER" mnemonicParsing="false" onAction="#showLiveGraph" prefHeight="115.0" prefWidth="299.0" style="-fx-background-color: gold;" text="Live Graph">
