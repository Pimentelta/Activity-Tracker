<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.csci360.activitytracker.MyJavaFX">
   <children>
      <GridPane prefHeight="314.0" prefWidth="405.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Hyperlink onAction="#onLiveGraphPress" text="Live Graph" />
            <Hyperlink onAction="#onNewSessionPress" prefHeight="47.0" prefWidth="75.0" text="New Session" GridPane.columnIndex="1" />
            <Hyperlink onAction="#onSetNewGoalPress" text="Set New Goal" GridPane.rowIndex="1" />
            <Hyperlink onAction="#onInputDataPress" text="Input Data" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Hyperlink onAction="#onViewDataPress" text="View Data" GridPane.rowIndex="2" />
            <Hyperlink onAction="#onModifyDeletePress" text="Modify/Delete" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
